{% extends 'base.html.twig' %}

{% block title %}Articulo{% endblock %}

{% block body %}
    <h1>Articulo</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ articulo.id }}</td>
            </tr>
            <tr>
                <th>Sku</th>
                <td>{{ articulo.sku }}</td>
            </tr>
            <tr>
                <th>Precio1</th>
                <td>{{ articulo.precio1 }}</td>
            </tr>
            <tr>
                <th>Precio2</th>
                <td>{{ articulo.precio2 }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{{ articulo.activo ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ articulo.descripcion }}</td>
            </tr>
            <tr>
                <th>UrlAmigable</th>
                <td>{{ articulo.urlAmigable }}</td>
            </tr>
            <tr>
                <th>FechahoraCreacion</th>
                <td>{{ articulo.fechahoraCreacion ? articulo.fechahoraCreacion|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>FechahoraEdicion</th>
                <td>{{ articulo.fechahoraEdicion ? articulo.fechahoraEdicion|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('articulo_index') }}">back to list</a>

    <a href="{{ path('articulo_edit', {'id': articulo.id}) }}">edit</a>

    {{ include('articulo/_delete_form.html.twig') }}
{% endblock %}
